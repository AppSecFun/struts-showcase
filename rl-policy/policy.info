policies {
    profile "package_profile" {
    
        inherit_rules "project_profile"
	    rl_scan_level 2
	
        processing
        {  
            filter {
                enabled   false
                author    "security@company.com"
                timestamp "2023-11-06T11:00:00+0000"
                reason    "Example of ignoring all policies on a file."
                matches   file
                pattern   "dwr-1.1.1.jar"
                policies {
                    SQ* {
                        blocker pass
                    }      
                }
            }
            filter {
                enabled   false
                author    "security@company.com"
                timestamp "2023-11-06T11:00:00+0000"
                reason    "Example of allowing an SQ ID on a file to pass policy."
                matches   path
                pattern   "*/dwr-1.1.1.jar"
                policies {
                    SQ31104 {
                        blocker pass
                    }      
                }
            }
            filter {
                enabled   false
                author    "security@company.com"
                timestamp "2023-11-06T11:00:00+0000"
                reason    "Example of triaging vulns to pass policy."
                matches   path
                pattern   *
                triaged {
                    CVE-2021-45046   mitigation-exists
                    CVE-2006-1547    vulnerable-code-absent
                }
            }
        }     
    }
}
